{
  "log": {
    "level": "info"
  },

  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "singbox0",
      "inet4_address": "172.19.0.1/30",
      "stack": "system",
      "auto_route": true,
      "strict_route": false,
      "sniff": true
    }
  ],

  "dns": {
    "servers": [
      {
        "tag": "dns_local",
        "type": "udp",
        "server": "223.5.5.5"
      },
      {
        "tag": "dns_fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "tag": "dns_remote",
        "type": "https",
        "server": "8.8.8.8"
      }
    ],
    "rules": [
      { "rule_set": "geosite-cn", "server": "dns_local" },
      { "query_type": ["A", "AAAA"], "server": "dns_fakeip" }
    ],
    "final": "dns_remote",
    "strategy": "prefer_ipv4",
    "reverse_mapping": true
  },

  "outbounds": [
    {
      "tag": "HK",
      "type": "urltest",
      "filter": "(?i)港|hk|hongkong"
    },
    {
      "tag": "JP",
      "type": "urltest",
      "filter": "(?i)日|jp|japan"
    },
    {
      "tag": "SG",
      "type": "urltest",
      "filter": "(?i)新|sg|singapore"
    },
    {
      "tag": "US",
      "type": "urltest",
      "filter": "(?i)美|us|united"
    },

    {
      "tag": "PROXY",
      "type": "selector",
      "outbounds": [
        "HK",
        "JP",
        "SG",
        "US"
      ]
    },

    {
      "tag": "DIRECT",
      "type": "direct"
    }
  ],

  "route": {
    "rules": [
      { "ip_is_private": true, "outbound": "DIRECT" },

      { "rule_set": "geosite-cn", "outbound": "DIRECT" },
      { "rule_set": "geoip-cn", "outbound": "DIRECT" },

      { "rule_set": "geosite-apple", "outbound": "DIRECT" },
      { "rule_set": "geosite-microsoft", "outbound": "DIRECT" },

      { "rule_set": "geosite-google", "outbound": "PROXY" },
      { "rule_set": "geosite-youtube", "outbound": "PROXY" },
      { "rule_set": "geosite-telegram", "outbound": "PROXY" },
      { "rule_set": "geosite-twitter", "outbound": "PROXY" },
      { "rule_set": "geosite-netflix", "outbound": "PROXY" },

      { "final": "PROXY" }
    ],

    "rule_set": [
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip-cn.srs"
      },
      {
        "tag": "geosite-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/apple.srs"
      },
      {
        "tag": "geosite-microsoft",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft.srs"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs"
      },
      {
        "tag": "geosite-youtube",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs"
      },
      {
        "tag": "geosite-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs"
      },
      {
        "tag": "geosite-twitter",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/twitter.srs"
      },
      {
        "tag": "geosite-netflix",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/netflix.srs"
      }
    ]
  }
}
